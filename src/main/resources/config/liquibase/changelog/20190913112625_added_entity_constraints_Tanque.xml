<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Tanque.
    -->
    <changeSet id="20190913112625-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="lote_id"
                                 baseTableName="tanque"
                                 constraintName="fk_tanque_lote_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lote"/>

        <addForeignKeyConstraint baseColumnNames="producto_id"
                                 baseTableName="tanque"
                                 constraintName="fk_tanque_producto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producto"/>

        <addForeignKeyConstraint baseColumnNames="empresa_id"
                                 baseTableName="tanque"
                                 constraintName="fk_tanque_empresa_id"
                                 referencedColumnNames="id"
                                 referencedTableName="empresa"/>

    </changeSet>
</databaseChangeLog>
