<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity MovimientoTanque.
    -->
    <changeSet id="20191016014659-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="tanque_id"
                                 baseTableName="movimiento_tanque"
                                 constraintName="fk_movimiento_tanque_tanque_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tanque"/>

        <addForeignKeyConstraint baseColumnNames="producto_id"
                                 baseTableName="movimiento_tanque"
                                 constraintName="fk_movimiento_tanque_producto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producto"/>

        <addForeignKeyConstraint baseColumnNames="lote_id"
                                 baseTableName="movimiento_tanque"
                                 constraintName="fk_movimiento_tanque_lote_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lote"/>

    </changeSet>
</databaseChangeLog>
