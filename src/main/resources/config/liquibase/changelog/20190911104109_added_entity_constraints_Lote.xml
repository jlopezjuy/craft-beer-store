<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Lote.
    -->
    <changeSet id="20190911104109-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="receta_id"
                                 baseTableName="lote"
                                 constraintName="fk_lote_receta_id"
                                 referencedColumnNames="id"
                                 referencedTableName="receta"/>

        <addForeignKeyConstraint baseColumnNames="empresa_id"
                                 baseTableName="lote"
                                 constraintName="fk_lote_empresa_id"
                                 referencedColumnNames="id"
                                 referencedTableName="empresa"/>

        <addForeignKeyConstraint baseColumnNames="producto_id"
                                 baseTableName="lote"
                                 constraintName="fk_lote_producto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producto"/>

        <addForeignKeyConstraint baseColumnNames="tanque_id"
                                 baseTableName="lote"
                                 constraintName="fk_lote_tanque_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tanque"/>

    </changeSet>
</databaseChangeLog>
