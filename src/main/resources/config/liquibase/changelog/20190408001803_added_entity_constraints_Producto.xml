<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Producto.
    -->
    <changeSet id="20190408001803-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="empresa_id"
                                 baseTableName="producto"
                                 constraintName="fk_producto_empresa_id"
                                 referencedColumnNames="id"
                                 referencedTableName="empresa"/>

        <addForeignKeyConstraint baseColumnNames="estilos_id"
                                 baseTableName="producto"
                                 constraintName="fk_producto_estilos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="estilos"/>

    </changeSet>
</databaseChangeLog>
